          <li><a href="/">About Us</a></li>
          <li><a href="/">Donors</a></li>
          <li><a href="/">Calendar</a></li>
          <li><a href="/">Members</a></li>
          <li><a href="/">The Team</a></li>
        </ol>
      </nav>
    </header>
    <main>
      {{#block "body"}}{{/block}}
    </main>
    <footer>
      <p class="copyright">&copy;2017 Vikings Robotics</p>
    </footer>
    {{#block "end-scripts"}}{{/block}}
